declare name "Standart Pitch Notation constants";
declare version "0.1";

ba = library("basics.lib");

oct(n) = (n + 1) * 12;

C(n) = 0 + oct(n);
D(n) = 2 + oct(n);
E(n) = 4 + oct(n);
F(n) = 5 + oct(n);
G(n) = 7 + oct(n);
A(n) = 9 + oct(n);
B(n) = 11 + oct(n);


C1 = C(1);
C2 = C(2);
C3 = C(3);
C4 = C(4);
C5 = C(5);
C6 = C(6);

D1 = D(1);
