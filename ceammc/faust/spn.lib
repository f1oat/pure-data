D4 = D(4);
D5 = D(5);
D6 = D(6);

E1 = E(1);
E2 = E(2);
E3 = E(3);
E4 = E(4);
E5 = E(5);
E6 = E(6);

F1 = F(1);
F2 = F(2);
F3 = F(3);
F4 = F(4);
F5 = F(5);
F6 = F(6);

G1 = G(1);
G2 = G(2);
G3 = G(3);
G4 = G(4);
G5 = G(5);
G6 = G(6);

A1 = A(1);
A2 = A(2);
A3 = A(3);
A4 = A(4);
