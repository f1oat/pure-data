<?xml version="1.0" encoding="utf-8"?>
<pddoc version="1.0">
    <object name="matrix.to_list">
        <title>matrix.to_list</title>
        <meta>
            <authors>
                <author>Serge Poltavski</author>
            </authors>
            <description>convert Matrix to list</description>
            <license>GPLv3</license>
            <library>matrix</library>
            <category>matrix</category>
            <version>0.1</version>
